{
    "post_product": {
        "model_spec": {
            "$schema": "http://json-schema.org/draft-04/schema#",
            "title": "post_product",
            "type": "object",
            "required": ["product_id", "seller_id"],
            "properties": {
                "product_id": {
                    "type": "string"
                },
                "seller_id": {
                    "type": "string"
                }

            }
        },
        "contentType": "application/json"
    },
    "post_product_comment": {
        "model_spec": {
            "$schema": "http://json-schema.org/draft-04/schema#",
            "title": "post_product_comment",
            "type": "object",
            "required": [
                "user",
                "user_id",
                "rating",
                "review",
                "title",
                "date",
                "review_id"
            ],
            "properties": {
                "user": {
                    "type": "string"
                },
                "user_id": {
                    "type": "string"
                },
                "rating": {
                    "type": "number"
                },
                "review": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "date": {
                    "type": "string"
                },
                "review_id": {
                    "type": "string"
                }
            }
        },
        "contentType": "application/json"
    },
    "put_order": {
        "model_spec": {
            "$schema": "http://json-schema.org/draft-04/schema#",
            "title": "put_order",
            "type": "object",
            "required": ["order_id", "status"],
            "properties": {
                "order_id": {
                    "type": "string"
                },
                "status_id": {
                    "type": "string"
                }
            }
        },
        "contentType": "application/json"
    }
}